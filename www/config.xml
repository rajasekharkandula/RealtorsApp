<?xml version='1.0' encoding='utf-8'?>
<widget id="com.narindia.app" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>NAR India</name>
    <description>
        National Association of Realtors - India
    </description>
    <author email="technology@narindia.com" href="http://narindia.org">
        NAR India Team
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#ee6e73" />
    <preference name="StatusBarStyle" value="blacktranslucent" />
    
	<icon src="img/logo.png" />	
	<platform name="android">
        <icon density="ldpi" src="www/img/logo.png" />
        <icon density="mdpi" src="www/img/logo.png" />
        <icon density="hdpi" src="www/img/logo.png" />
        <icon density="xhdpi" src="www/img/logo.png" />
        <icon density="xxhdpi" src="www/img/logo.png" />
        <icon density="xxxhdpi" src="www/img/logo.png" />
        <splash density="port-ldpi" src="www/img/logo.png" />
        <splash density="port-mdpi" src="www/img/logo.png" />
        <splash density="port-hdpi" src="www/img/logo.png" />
        <splash density="port-xhdpi" src="www/img/logo.png" />
    </platform>
	
	<platform name="ios">
		<icon height="57" platform="ios" src="res/icons/ios/Icon-57.png" width="57" />
		<icon height="114" platform="ios" src="res/icons/ios/Icon-114.png" width="114" />
		<icon height="40" platform="ios" src="res/icons/ios/Icon-40.png" width="40" />
		<icon height="80" platform="ios" src="res/icons/ios/Icon-80.png" width="80" />
		<icon height="50" platform="ios" src="res/icons/ios/Icon-50.png" width="50" />
		<icon height="100" platform="ios" src="res/icons/ios/Icon-100.png" width="100" />
		<icon height="60" platform="ios" src="res/icons/ios/Icon-60.png" width="60" />
		<icon height="120" platform="ios" src="res/icons/ios/Icon-120.png" width="120" />
		<icon height="180" platform="ios" src="res/icons/ios/Icon-180.png" width="180" />
		<icon height="72" platform="ios" src="res/icons/ios/Icon-72.png" width="72" />
		<icon height="144" platform="ios" src="res/icons/ios/Icon-144.png" width="144" />
		<icon height="76" platform="ios" src="res/icons/ios/Icon-76.png" width="76" />
		<icon height="152" platform="ios" src="res/icons/ios/Icon-152.png" width="152" />
		<icon height="29" platform="ios" src="res/icons/ios/Icon-29.png" width="29" />
		<icon height="58" platform="ios" src="res/icons/ios/Icon-58.png" width="58" />
		<icon height="87" platform="ios" src="res/icons/ios/Icon-87.png" width="87" />
		<icon height="1024" platform="ios" src="res/icons/ios/Icon-1024.png" width="1024" />
		<icon height="512" platform="ios" src="res/icons/ios/Icon-512.png" width="512" />
	
		<gap:splash height="1136" platform="ios" src="res/screens/ios/640_1136.png" width="640" />
		<gap:splash height="1334" platform="ios" src="res/screens/ios/750_1334.png" width="750" />
		<gap:splash height="2208" platform="ios" src="res/screens/ios/1242_2208.png" width="1242" />
		<gap:splash height="1242" platform="ios" src="res/screens/ios/2208_1242.png" width="2208" />
		<gap:splash height="1536" platform="ios" src="res/screens/ios/2048_1536.png" width="2048" />
		<gap:splash height="768" platform="ios" src="res/screens/ios/1024_768.png" width="1024" />
		<gap:splash height="2048" platform="ios" src="res/screens/ios/1536_2048.png" width="1536" />
		<gap:splash height="1024" platform="ios" src="res/screens/ios/768_1024.png" width="768" />
		<gap:splash height="960" platform="ios" src="res/screens/ios/640_960.png" width="640" />
		<gap:splash height="480" platform="ios" src="res/screens/ios/320_480.png" width="320" />
	</platform>
	
    <access origin="*" />
    <allow-intent href="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-console" spec="~1.0.3" />
    <plugin name="cordova-plugin-dialogs" spec="~1.2.1" />
    <plugin name="cordova-plugin-statusbar" spec="~2.1.3" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.1" />
    <plugin name="phonegap-plugin-push" spec="~1.8.0">
        <variable name="SENDER_ID" value="XXXXXXX" />
    </plugin>
</widget>
